package co.edu.ue.service;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;

import co.edu.ue.model.Alumno;
import dao.AlumnoDAOI;

public class AlumnoService implements AlumnoServiceI {

	@Autowired
	AlumnoDAOI dao;
	
	@Override
	public boolean addAlumno(Alumno alumno) {
		if (dao.searchAlumno(alumno.getCodigo())==null) {
			dao.addAlumno(alumno);
		}
		return true;
	}

	@Override
	public List<Alumno> getAllAlumno() {
		// TODO Auto-generated method stub
		return dao.getAllAlumno();
	}

	@Override
	public void uppdateAlumno(Alumno alumno) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public boolean delecIdAlumno(int idAlumno) {
		// TODO Auto-generated method stub
		return false;
	}

}
